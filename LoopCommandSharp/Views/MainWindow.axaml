<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:LoopCommandSharp.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="100"
		Width="200" Height="100"
        x:Class="LoopCommandSharp.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="LoopCommandSharp">

    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>

	<StackPanel>
		<Button Name="Mint" Width="150" HorizontalAlignment="Center" Click="ShowMintNftWindow">Mint NFT</Button>
        <Button Width="150" HorizontalAlignment="Center" Click="ShowSettingsWindow">Settings</Button>
</StackPanel>
</Window>
